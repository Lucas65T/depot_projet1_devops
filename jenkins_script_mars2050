pipeline {
    agent any
        build-job {
            stage('BUILD') {
                steps {
                    script {
                        echo "Lancement - mars 2050"
                        sleep 30
                        echo "Traitement"
                    }
                }
            }
        }
        first-test-job {
             stage('TEST') {
                 steps {
                    script {
                        echo "Réduire la consommation globale de carburant de 1 %... This will take about 60 seconds."
                        sleep 60
                        echo "Code coverage is 90%"
                    }
                }
            }
        }
        second-test-job {
             stage('TEST') {
                 steps {
                    script {
                        echo "Mettre à jour la plage des dates... This will take about 30 seconds."
                        sleep 30
                        echo "No dates found !"
                    }
                }
            }
        }
        deploy-job {
             stage('DEPLOY') {
                 steps {
                    script {
                        echo "Augmenter le nombre de lancement de navette spatiales... This will take about 30 seconds."
                        sleep 30
                        echo "Nombre de navettes augmenté. Prêt pour le lencement... Cela va prendre un certain temps."
                        sleep 10
                        echo "Lancement réussi ! C'est un petit pas pour l'homme, et un grand pas pour l'humanité !"
                    }
                }
             }
        }
   }
